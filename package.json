{
  "name": "nifty-thrifty",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:structure": "node scripts/test-new-structure.js",
    "# Deployment scripts": "",
    "deploy:scroll-whatsapp": "node scripts/deployment/whatsapp-scroll.js",
    "deploy:update-website": "bash scripts/deployment/update-website.sh",
    "deploy:update-website-deploy": "bash scripts/deployment/update-website.sh --deploy",
    "deploy:update-website-full": "bash scripts/deployment/update-website.sh --scroll",
    "deploy:update-website-scroll-only": "bash scripts/deployment/update-website.sh --scroll-only",
    "deploy:update-waha": "bash scripts/deployment/waha-update.sh",
    "deploy:update-waha-deploy": "bash scripts/deployment/waha-update.sh --deploy",
    "deploy:update-waha-restart": "bash scripts/deployment/waha-update.sh --restart-waha",
    "deploy:update-waha-full": "bash scripts/deployment/waha-update.sh --restart-waha --deploy",
    "deploy:restart-waha": "docker stop $(docker ps -q --filter 'ancestor=devlikeapro/waha') && docker run -d -p 3001:3000 devlikeapro/waha",
    "# Import scripts": "",
    "import:waha": "node scripts/import/waha-import.js",
    "import:waha-verbose": "node scripts/import/waha-import.js --verbose --days 7",
    "import:waha-full": "node scripts/import/waha-import.js --verbose --upload-images --check-images --days 7",
    "import:waha-smart": "node scripts/import/waha-import.js --verbose",
    "import:waha-force-days": "node scripts/import/waha-import.js --verbose --ignore-last-date --days 7",
    "import:waha-gemini": "node scripts/import/waha-gemini-import.js",
    "import:waha-gemini-verbose": "node scripts/import/waha-gemini-import.js --verbose",
    "import:waha-gemini-full": "node scripts/import/waha-gemini-import.js --verbose --upload-images --check-images",
    "import:update-waha-gemini": "npm run deploy:restart-waha && npm run import:waha-gemini-full",
    "import:update-waha-gemini-deploy": "npm run import:update-waha-gemini && npm run vercel-deploy",
    "# Image processing": "",
    "image:process": "node scripts/image-handling/waha-image-handler.js",
    "image:upload": "node scripts/image-handling/waha-image-handler.js --verbose --upload",
    "# Maintenance scripts": "",
    "maintenance:find-duplicates": "node scripts/maintenance/duplicate-finder.js",
    "maintenance:find-duplicates-cross-group": "node scripts/maintenance/duplicate-finder.js --threshold 0.5",
    "maintenance:find-duplicate-images": "node scripts/maintenance/duplicate-finder.js --images",
    "maintenance:remove-duplicates": "node scripts/maintenance/duplicate-finder.js --remove --threshold 0.7",
    "maintenance:remove-duplicates-strict": "node scripts/maintenance/duplicate-finder.js --remove --threshold 0.9",
    "maintenance:update-phone-numbers": "node scripts/maintenance/update-phone-numbers.js",
    "maintenance:update-phone-numbers-dry-run": "node scripts/maintenance/update-phone-numbers.js --dry-run --verbose",
    "maintenance:find-duplicates-by-phone": "node scripts/maintenance/duplicate-finder.js --use-phone-numbers --no-use-db-queries",
    "maintenance:find-duplicates-db": "node scripts/maintenance/duplicate-finder.js --use-db-queries",
    "maintenance:remove-duplicates-by-phone": "node scripts/maintenance/duplicate-finder.js --use-phone-numbers --no-use-db-queries --remove",
    "# TypeScript scripts": "",
    "ts-scripts:process-whatsapp": "ts-node src/scripts/processNiftyThrifty01Years.ts",
    "ts-scripts:enrich-listings": "ts-node src/scripts/enrichListings.ts",
    "ts-scripts:extract-whatsapp": "ts-node src/scripts/extractWhatsAppMessages.ts",
    "ts-scripts:filter-listings": "ts-node src/scripts/filterListings.ts",
    "ts-scripts:convert-listings": "ts-node src/scripts/convertListingsToAppFormat.ts",
    "ts-scripts:integrate-listings": "ts-node src/scripts/integrateNiftyThriftyListings.ts",
    "ts-scripts:import-whatsapp-group": "ts-node src/scripts/importWhatsAppGroup.ts",
    "ts-scripts:update-listings-without-images": "ts-node src/scripts/updateListingsWithoutImages.ts",
    "ts-scripts:manually-review-listings": "ts-node src/scripts/manuallyReviewListingsWithoutImages.ts",
    "# Database scripts": "",
    "db:setup-supabase": "node src/scripts/setupSupabase.js",
    "db:migrate-to-supabase": "node src/scripts/migrateToSupabase.js",
    "db:run-migration": "node scripts/database/run-migration.js",
    "db:add-multi-message-columns": "node scripts/database/run-migration.js add_multi_message_columns.sql",
    "db:switch-to-uncategorised": "echo 'Please run the SQL script scripts/database/switch-to-uncategorised.sql in the Supabase SQL Editor'",
    "# Categorization scripts": "",
    "category:apply": "node scripts/categorization/apply-categories.js",
    "category:with-llm": "node scripts/categorization/categorize-with-llm.js",
    "category:uncategorised-with-llm": "node scripts/categorization/categorize-with-llm.js --non-interactive --service=openai --live",
    "category:uncategorised-dry-run": "node scripts/categorization/categorize-with-llm.js --non-interactive --service=openai",
    "category:uncategorised-mock": "node scripts/categorization/categorize-with-llm.js --non-interactive --service=mock --live",
    "category:update-iso-flags": "node scripts/categorization/update-iso-flags.js",
    "# ISO classification scripts": "",
    "iso:classify-with-llm": "node scripts/categorization/classify-iso-with-llm.js",
    "iso:classify-live": "node scripts/categorization/classify-iso-with-llm.js --non-interactive --service=openai --live",
    "iso:classify-dry-run": "node scripts/categorization/classify-iso-with-llm.js --non-interactive --service=openai",
    "iso:classify-mock": "node scripts/categorization/classify-iso-with-llm.js --non-interactive --service=mock --live",
    "iso:delete-conversations": "node scripts/categorization/classify-iso-with-llm.js --non-interactive --service=openai --live",
    "iso:delete-conversations-dry-run": "node scripts/categorization/classify-iso-with-llm.js --non-interactive --service=openai",
    "iso:cleanup-listings": "node scripts/categorization/classify-iso-with-llm.js --non-interactive --service=openai --live --limit=200",
    "# Vercel deployment": "",
    "vercel-deploy": "vercel --prod"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.49.1",
    "@types/uuid": "^10.0.0",
    "axios": "^1.8.2",
    "classnames": "^2.5.1",
    "commander": "^12.0.0",
    "date-fns": "^3.3.1",
    "next": "^14.1.0",
    "pg": "^8.13.3",
    "puppeteer": "^22.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "string-similarity": "^4.0.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.17",
    "dotenv": "^16.4.7",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
